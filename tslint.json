{
  "extends": [
    "tslint-react",
    "tslint-config-standard"
  ],
  "defaultSeverity": "warning",
  "rules": {
    "jsx-no-multiline-js": false,
    "jsx-boolean-value": false,
    "jsx-no-lambda": false,
    "no-use-before-declare": false,
    "trailing-comma": false,
    "space-before-function-paren": false,
    "semicolon": [
      true,
      "never"
    ]
  }
}
