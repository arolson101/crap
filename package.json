{
  "name": "crap",
  "version": "0.1.0",
  "author": "arolson101",
  "description": "crap",
  "private": true,
  "dependencies": {
    "electron-is-dev": "^0.3.0",
    "electron-window-state": "^4.1.1"
  },
  "build": {
    "appId": "com.github.arolson101.crap",
    "files": [
      "build/**/*",
      "!build/**/*.map",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    }
  },
  "homepage": "http://github.com/arolson101/crap/",
  "main": "public/electron.js",
  "scripts": {
    "analyze-web": "source-map-explorer build/static/js/main.*",
    "coverage": "npm test -- --coverage",
    "preanalyze-web": "npm run build-web",
    "web": "react-app-rewired start --scripts-version react-scripts-ts",
    "serve": "cross-env BROWSER=none react-app-rewired start --scripts-version react-scripts-ts",
    "electron": "electron .",
    "electron-pack": "build --em.main=build/electron.js --dir",
    "preelectron-pack": "npm run build-web",
    "build-web": "react-app-rewired build --scripts-version react-scripts-ts",
    "test:cra": "react-app-rewired test --scripts-version react-scripts-ts --env=jsdom",
    "test": "jest",
    "eject-web": "react-app-rewired eject --scripts-version react-scripts-ts",
    "storybook-web": "start-storybook -p 6006 -c .storybook/web -s ./node_modules",
    "build-storybook-web": "build-storybook -c .storybook/web -s ./node_modules",
    "start-native": "react-native-scripts-ts start",
    "eject-native": "react-native-scripts-ts eject",
    "android": "react-native-scripts-ts android",
    "ios": "react-native-scripts-ts ios",
    "storybook-native": "storybook start -p 7007 -c .storybook/native",
    "start": "npm run serve",
    "storybook": "npm run storybook-web",
    "stylelint": "stylelint \"src/**/*.tsx\""
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.3.13",
    "@storybook/addon-links": "^3.3.13",
    "@storybook/addons": "^3.3.13",
    "@storybook/react": "^3.3.13",
    "@storybook/react-native": "^3.3.13",
    "@types/bson": "^1.0.6",
    "@types/expo": "^24.0.3",
    "@types/history": "^4.6.2",
    "@types/jest": "^22.1.2",
    "@types/lodash-es": "^4.17.0",
    "@types/node": "^9.4.5",
    "@types/pako": "^1.0.0",
    "@types/prop-types": "^15.5.2",
    "@types/react": "^16.0.36",
    "@types/react-dom": "^16.0.3",
    "@types/react-form": "^2.12.0",
    "@types/react-helmet": "^5.0.3",
    "@types/react-hot-loader": "^3.0.5",
    "@types/react-intl": "^2.3.4",
    "@types/react-native": "^0.52.8",
    "@types/react-native-vector-icons": "^4.4.2",
    "@types/react-navigation": "^1.0.31",
    "@types/react-redux": "^5.0.14",
    "@types/react-router-dom": "^4.2.3",
    "@types/react-router-native": "^4.0.3",
    "@types/react-router-redux": "^5.0.12",
    "@types/react-test-renderer": "^16.0.0",
    "@types/recompose": "^0.24.5",
    "@types/redux-devtools-extension": "^2.13.2",
    "@types/redux-mock-store": "^0.0.13",
    "@types/redux-thunk": "^2.1.0",
    "@types/rx": "^4.1.1",
    "@types/shortid": "^0.0.29",
    "@types/storybook__addon-actions": "^3.0.1",
    "@types/storybook__addon-knobs": "^3.2.2",
    "@types/storybook__addon-links": "^3.0.2",
    "@types/storybook__addon-options": "^3.2.0",
    "@types/storybook__react": "^3.0.6",
    "babel-core": "^6.26.0",
