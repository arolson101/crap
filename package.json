{
  "name": "crap",
  "version": "0.0.1",
  "author": "arolson101",
  "private": true,
  "build": {
    "appId": "com.github.arolson101.crap",
    "files": [
      "build/**/*",
      "!build/**/*.map",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    }
  },
  "scripts": {
    "analyze-web": "source-map-explorer build/static/js/main.*",
    "coverage": "npm test -- --coverage",
    "preanalyze-web": "npm run build-web",
    "web": "react-app-rewired start",
    "serve": "cross-env BROWSER=none react-app-rewired start",
    "electron": "electron .",
    "preelectron": "electron-builder install-app-deps",
    "electron-pack": "build -c.extraMetadata.main=build/electron.js --dir",
    "preelectron-pack": "npm run build-web",
    "build-web": "react-app-rewired build",
    "test:cra": "react-app-rewired test --env=jsdom",
    "test": "jest",
    "eject-web": "react-app-rewired eject",
    "storybook-web": "start-storybook -p 6006 -c .storybook/web -s ./node_modules",
    "build-storybook-web": "build-storybook -c .storybook/web -s ./node_modules",
    "start-native": "react-native start",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "preios": "cd ios && pod install",
    "storybook-native": "storybook start -p 7007 -c .storybook/native",
    "start": "npm run serve",
    "storybook": "npm run storybook-web",
    "pregenerate": "rimraf temp/graphdoc && mkdirp temp",
    "generate": "npm run generate:schema && npm run graphdoc && npm run generate:queries && npm run generate:mutations && npm run generate:dts",
    "generate:schema": "ts-node --transpileOnly --project tsconfig.test.json --ignore \"/sql.js/\" ./genSchema.ts",
    "generate:queries": "gql-gen --require ts-node/register --schema temp/schema.json --template graphql-codegen-typescript-template --out src/db/queries/queries-types.ts \"src/db/queries/queries.graphql\"",
    "generate:mutations": "gql-gen --require ts-node/register --schema temp/schema.json --template graphql-codegen-typescript-template --out src/db/mutations/mutations-types.ts \"src/db/mutations/mutations.graphql\"",
    "generate:dts": "ts-node --transpileOnly --project tsconfig.test.json ./genGraphqlDts.ts \"src/db/queries/queries.graphql\" \"src/db/mutations/mutations.graphql\"",
    "graphdoc": "graphdoc --force -s ./temp/schema.json -o ./temp/graphdoc",
    "lint": "tslint --fix \"{src,stories}/**/*{.ts,.tsx}\" --project tsconfig.json",
    "react-devtools": "react-devtools"
  },
  "dependencies": {
    "electron-is-dev": "^1.0.1",
    "electron-window-state": "^5.0.1",
    "node-pre-gyp": "^0.11.0",
    "sqlite3": "^4.0.2",
    "typeorm": "https://github.com/arolson101/typeorm.git#with_build"
