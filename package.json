{
  "name": "crap",
  "version": "0.1.0",
  "author": "arolson101",
  "description": "crap",
  "private": true,
  "dependencies": {
    "electron-is-dev": "^0.3.0",
    "electron-window-state": "^4.1.1"
  },
  "build": {
    "appId": "com.github.arolson101.crap",
    "files": [
      "build/**/*",
      "!build/**/*.map",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    }
  },
  "homepage": "http://github.com/arolson101/crap/",
  "main": "public/electron.js",
  "scripts": {
    "analyze-web": "source-map-explorer build/static/js/main.*",
    "coverage": "npm test -- --coverage",
    "preanalyze-web": "npm run build-web",
    "web": "react-app-rewired start",
    "serve": "cross-env BROWSER=none react-app-rewired start",
    "electron": "electron .",
    "electron-pack": "build -c.extraMetadata.main=build/electron.js --dir",
    "preelectron-pack": "npm run build-web",
    "build-web": "react-app-rewired build",
    "test:cra": "react-app-rewired test --env=jsdom",
    "test": "jest",
    "eject-web": "react-app-rewired eject",
    "storybook-web": "start-storybook -p 6006 -c .storybook/web -s ./node_modules",
    "build-storybook-web": "build-storybook -c .storybook/web -s ./node_modules",
    "start-native": "react-native-scripts-ts start",
    "eject-native": "react-native-scripts-ts eject",
    "android": "react-native-scripts-ts android",
    "ios": "react-native-scripts-ts ios",
    "storybook-native": "storybook start -p 7007 -c .storybook/native",
    "start": "npm run serve",
    "storybook": "npm run storybook-web",
    "stylelint": "stylelint \"src/**/*.tsx\"",
    "pregenerate": "mkdirp temp && apollo-codegen introspect-schema ./src/db/schema/schema.graphql --output temp/schema.json",
    "generate": "npm run generate:schema && npm run generate:queries && npm run generate:mutations",
    "generate:queries": "apollo-codegen generate \"src/db/queries/*.tsx\" --schema temp/schema.json --target typescript --output src/db/queries/query-types.ts",
    "generate:mutations": "apollo-codegen generate \"src/db/mutations/*.tsx\" --schema temp/schema.json --target typescript --output src/db/mutations/mutation-types.ts",
    "generate:schema": "gql-gen --template typescript --file ./temp/schema.json --out ./src/db/schema/schema-types.ts ./src/db/schema/schema.graphql"
  },
  "devDependencies": {
    "@expo/vector-icons": "^6.3.1",
    "@storybook/addon-actions": "^3.4.0",
    "@storybook/addon-knobs": "^3.4.0",
    "@storybook/addon-links": "^3.4.0",
    "@storybook/addons": "^3.4.0",
    "@storybook/react": "^3.4.0",
    "@storybook/react-native": "^3.4.0",
    "@types/bson": "^1.0.7",
    "@types/expo": "^25.0.3",
    "@types/expo__vector-icons": "^6.2.1",
    "@types/graphql": "^0.13.0",
    "@types/history": "^4.6.2",
    "@types/inline-style-prefixer": "^3.0.1",
    "@types/jest": "^22.2.2",
    "@types/lodash-es": "^4.17.0",
    "@types/node": "^9.6.2",
    "@types/prop-types": "^15.5.2",
    "@types/randomcolor": "^0.4.3",
    "@types/react": "^16.3.6",
    "@types/react-dom": "^16.0.4",
    "@types/react-form": "^2.16.0",
    "@types/react-helmet": "^5.0.5",
    "@types/react-hot-loader": "^3.0.6",
    "@types/react-intl": "^2.3.6",
    "@types/react-native": "^0.55.0",
    "@types/react-native-vector-icons": "^4.6.0",
    "@types/react-navigation": "^1.5.5",
    "@types/react-router-dom": "^4.2.6",
    "@types/react-router-native": "^4.2.1",
    "@types/react-test-renderer": "^16.0.1",
    "@types/recompose": "^0.24.7",
    "@types/rx": "^4.1.1",
    "@types/shortid": "^0.0.29",
    "@types/storybook__addon-actions": "^3.0.3",
    "@types/storybook__addon-knobs": "^3.3.0",
    "@types/storybook__addon-links": "^3.3.0",
    "@types/storybook__addon-options": "^3.2.0",
    "@types/storybook__react": "^3.0.7",
    "apollo-boost": "^0.1.4",
    "apollo-client": "^2.2.8",
    "apollo-codegen": "^0.19.1",
    "apollo-link": "^1.2.1",
    "apollo-server-express": "^1.3.4",
    "apollo-storybook-react": "^0.1.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
