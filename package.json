{
  "name": "crap",
  "version": "0.1.0",
  "author": "arolson101",
  "description": "crap",
  "private": true,
  "dependencies": {
    "electron-is-dev": "^0.3.0",
    "electron-window-state": "^4.1.1"
  },
  "build": {
    "appId": "com.github.arolson101.crap",
    "files": [
      "build/**/*",
      "!build/**/*.map",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    }
  },
  "homepage": "http://github.com/arolson101/crap/",
  "main-react": "./node_modules/react-native-scripts-ts/build/bin/crna-entry.js",
  "main": "public/electron.js",
  "scripts": {
    "analyze-web": "source-map-explorer build/static/js/main.*",
    "preanalyze-web": "npm run build-web",
    "web": "react-scripts-ts start",
    "serve": "cross-env BROWSER=none react-scripts-ts start",
    "electron": "electron .",
    "electron-pack": "build --em.main=build/electron.js --dir",
    "preelectron-pack": "npm run build-web",
    "build-web": "react-scripts-ts build",
    "test": "react-scripts-ts test --env=jsdom",
    "eject-web": "react-scripts-ts eject",
    "storybook-web": "start-storybook -p 6006 -c .storybook/web",
    "build-storybook-web": "build-storybook -c .storybook/web",
    "start-native": "react-native-scripts-ts start",
    "eject-native": "react-native-scripts-ts eject",
    "android": "react-native-scripts-ts android",
    "ios": "react-native-scripts-ts ios",
    "storybook-native": "storybook start -p 7007 -c .storybook/native",
    "start": "npm run serve",
    "storybook": "npm run storybook-web"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.3.9",
    "@storybook/addon-links": "^3.3.9",
    "@storybook/addons": "^3.3.9",
    "@storybook/react": "^3.3.9",
    "@storybook/react-native": "^3.3.9",
    "@types/jest": "^22.0.1",
    "@types/node": "^9.3.0",
    "@types/prop-types": "^15.5.2",
    "@types/react": "^16.0.34",
    "@types/react-dom": "^16.0.3",
    "@types/react-native": "^0.52.0",
    "@types/react-redux": "^5.0.14",
    "@types/react-test-renderer": "^16.0.0",
    "@types/redux-devtools-extension": "^2.13.2",
    "@types/rx": "^4.1.1",
    "@types/storybook__addon-actions": "^3.0.1",
    "@types/storybook__addon-knobs": "^3.2.1",
    "@types/storybook__addon-links": "^3.0.2",
    "@types/storybook__addon-options": "^3.2.0",
    "@types/storybook__react": "^3.0.6",
    "babel-core": "^6.26.0",
    "cross-env": "^5.1.3",
    "devtron": "^1.4.0",
    "electron": "^1.7.10",
    "electron-builder": "^19.54.0",
    "electron-devtools-installer": "^2.2.3",
    "electron-react-devtools": "^0.5.3",
    "expo": "^24.0.2",
    "prop-types": "^15.6.0",
    "raf": "^3.4.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-native": "^0.52.0",
    "react-native-scripts-ts": "^1.7.0",
    "react-native-typescript-transformer": "^1.2.1",
    "react-native-web": "^0.3.1",
    "react-redux": "^5.0.6",
    "react-redux-typescript": "^3.0.0-rc.3",
    "react-scripts-ts": "^2.12.0",
    "react-test-renderer": "^16.2.0",
    "redux": "^3.7.2",
    "redux-devtools-extension": "^2.13.2",
    "redux-observable": "^0.17.0",
    "rxjs": "^5.5.6",
    "source-map-explorer": "^1.5.0",
    "ts-jest": "^22.0.1",
    "ts-loader": "^3.2.0",
    "tslib": "^1.8.1",
    "tslint-react": "^3.4.0",
    "typesafe-actions": "^1.1.2",
    "typescript": "^2.6.2"
  }
}
